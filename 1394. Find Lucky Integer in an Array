class Solution {
public:
    int findLucky(vector<int>& arr) {
    map<int,int>prefix;
    int res=-1;
    for(int i=0;i<arr.size();i++)
    {
       prefix[arr[i]]++;
    }
    for(auto i:prefix)
    {
        if(i.first==i.second && i.first>res)
        {
            res=i.first;
        }
    }
    return res;
    }
};
